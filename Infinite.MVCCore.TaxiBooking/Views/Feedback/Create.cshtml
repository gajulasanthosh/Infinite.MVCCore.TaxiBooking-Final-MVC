@model FeedbackViewModel
@{
    ViewData["Title"] = "Create";
}

<h1>Please provide Feedback</h1>

<form asp-controller="Feedback" asp-action="Create" method="post">
    <div class="row">
        <div class="col-md-4">
        <div class="mb-2">
        <label asp-for="feedback" class="form-label"></label>
        <input type="text" asp-for="feedback" class="form-control" />
        <span asp-validation-for="feedback"></span></div></div>
    <div class="mt-2" d-grid>
        <input type="submit" class="btn btn-primary" value="Submit" />
        </div>
        </div>
</form>